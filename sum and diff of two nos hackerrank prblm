/* https://www.hackerrank.com/challenges/sum-numbers-c/problem */
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b;
    float c,d;
    scanf("%d%d%f%f",&a,&b,&c,&d);
    printf("%d %d",a+b,a-b);
    printf("\n%0.1f %0.1f",c+d,c-d);
    
    return 0;
}
